	libraryPath	"../libraries/library.C2H4/OutputXML";

	//- Minimum value of scalar dissipation rate [Hz]
	chiMinimum      1e-5;	

	//- Uniform value of scalar dissipation rate (only for starting simulations)
	chiUniform 	false;

